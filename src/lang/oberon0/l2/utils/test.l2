MODULE test;

CONST
	c = 20;

VAR
  x : INTEGER;
  y : BOOLEAN;

BEGIN
	x := c;
	WHILE x = c DO
		x := 5;
		IF x > 3 THEN
			y := x * c
		ELSE
			y := x DIV c
		END
	END;

	CASE i OF
	1: 
		x := i + 1
	2: x := i * y; y := 2 * x
	ELSE x := 1 + 2
	END;
	
	FOR i := 1 TO 50 DO
		x := i
	END;
	REPEAT
		x := x
	UNTIL x = x
		
END test.